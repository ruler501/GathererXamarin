<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:MtSparked.UI.Views.Search"
             x:Class="MtSparked.UI.Views.Search.DomainPage"
             x:Name="SearchPage">
    <ContentPage.ToolbarItems>
        <ToolbarItem Text="Clear" Clicked="Clear" />
        <ToolbarItem Text="Confirm" Clicked="Confirm" />
    </ContentPage.ToolbarItems>
    <ContentPage.Content>
        <ScrollView>
            <local:DomainGroupCell x:Name="RootGroup"/>
        </ScrollView>
    </ContentPage.Content>
</ContentPage>